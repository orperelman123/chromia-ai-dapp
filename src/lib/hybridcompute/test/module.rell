module;

import hc: ^;

operation submit_compute_request(id: text, type: text, input: gtv) {
    hc.submit_compute_request(id, type, input);
}

query fetch_requests(): list<struct<hc.computation>> = hc.computation @* { } ( $.to_struct() );

query fetch_compute_result(id: text): hc.compute_result? = hc.fetch_compute_result(id);
